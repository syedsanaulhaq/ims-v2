    "prod:start": "echo Starting PRODUCTION environment... && npm run build && concurrently \"npm run backend\" \"npm run preview\"",
    "status": "node scripts/check-environment.js",
    "switch:dev": "echo Switching to DEVELOPMENT... && npm run dev:start",
